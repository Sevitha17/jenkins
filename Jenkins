pipeline {
    agent any

    stages {
        stage('Clone Code') {
            steps {
                git branch: 'main', url: 'http://51.20.127.149:8080/github-webhook/'
            }
        }

        stage('Build') {
            steps {
                echo "Building the Project..."
            }
        }

        stage('Deploy') {
            steps {
                echo "Deploying the Project..."
            }
        }
    }
}
